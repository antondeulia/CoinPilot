{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/node" }
    ],
    "functions": {
      "dist/**/*.js": {
        "runtime": "nodejs24.x",
        "memory": 1024,
        "maxDuration": 30
      }
    },
    "routes": [
      { "src": "/(.*)", "dest": "/dist/main.js" }
    ]
  }